{
    "name": "Calendar Countdown",
    "description": "Countdown time remaining in the day, week, month, or year.",
    "version": "1.1.2",
    "manifest_version": 2,
    "icons": {
        "48": "extension-icons/Clock-48.png",
        "128": "extension-icons/Clock-128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
